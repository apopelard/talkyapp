<div class="panel panel-primary round">
  <div class="panel-heading top_round">
    <h2>All Users</h2>
  </div>
  <div class="panel-body">
    <div class="row page">
      <%= paginate @users %>
    </div>
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-md-6">
          <%= user_profile(user,current_user) %>
        </div>
      <% end %>
    </div>
  </div>
</div>


